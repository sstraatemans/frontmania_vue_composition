{
  "extends": ["plugin:vue/essential", "eslint:recommended"],
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": 0,
    "semicolon": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "arrow-return-shorthand": [true, "multiline"]
  }
}
